/// Algoritmo NOMBRE DEL PROGRAMA
Algoritmo MiPrimerPrograma
	/// Definir IDENTIFICADOR como TIPO DE DATO
	Definir miNombre como caracter
	/// Asignacion
	miNombre = ""
	
	/// Escribir EXPRESIONES O CONSTANTES A IMPRIMIR SEPARADAS POR COMA
	Escribir "Escribe tu nombre"
	
	/// Leer VARIABLES SEPARADAS POR COMA
	Leer miNombre
	
	Escribir "Hola soy", miNombre
	
FinAlgoritmo
